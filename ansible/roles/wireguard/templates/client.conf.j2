[Interface]
PrivateKey = {{ wireguard_client_private_key_content.content | b64decode | trim }}
Address = {{ wireguard_peer1 }}

[Peer]
PublicKey = {{ wireguard_server_public_key_gen.stdout }}
Endpoint = {{ wireguard_server_ip.stdout }}:{{ wireguard_port }}
AllowedIPs = 0.0.0.0/0, ::/0
PersistentKeepalive = 25
